{
  "name": "rocket-launch-live-client",
  "version": "0.1.1",
  "description": "A Node.JS client for interacting with the RocketLaunch.Live API",
  "main": "dist/index.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "dev": "nodemon ./dist/test.js --ignore 'dist/**' -e ts --exec \"npm run compile\"",
    "compile": "npm run clear && tsc && node ./dist/test.js",
    "build": "tsc",
    "start": "node ./dist/test.js",
    "prepublish": "tsc",
    "test": "mocha --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mendahu/rocket-launch-live-client.git"
  },
  "keywords": [
    "rocket",
    "rockets",
    "launch",
    "launches"
  ],
  "author": "Jake Robins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mendahu/rocket-launch-live-client/issues"
  },
  "homepage": "https://github.com/mendahu/rocket-launch-live-client#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0",
    "nock": "^13.3.0",
    "nodemon": "^2.0.20",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "types": "dist/index.d.ts"
}
